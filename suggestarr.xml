<?xml version="1.0"?>
<Container version="2">
  <Name>SuggestArr</Name>
  <Repository>ciuse99/suggestarr:latest</Repository>
  <Registry>https://hub.docker.com/r/ciuse99/suggestarr</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/giuseppe99barchetta/suggestarr/issues</Support>
  <Project>https://github.com/giuseppe99barchetta/suggestarr</Project>
  <Overview>Automatically request suggested movies and TV shows to Jellyseerr based on recently watched.</Overview>
  <Category>Downloaders: MediaApp:Video</Category>
  <WebUI>http://[IP]:[PORT:5000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/JamsRepos/Unraid-Templates/master/templates/suggestarr.xml</TemplateURL>
  <Icon>https://github.com/giuseppe99barchetta/SuggestArr/blob/main/unraid/logo.png?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1730019804</DateInstalled>
  <DonateText>Feeling generous?  Consider sponsoring me on GitHub!</DonateText>
  <DonateLink>https://github.com/sponsors/JamsRepos</DonateLink>
  <Requires/>
  <Config Name="Web UI Port" Target="5000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">5000</Config>
  <Config Name="config" Target="/app/config/config_files" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/suggestarr</Config>
</Container>